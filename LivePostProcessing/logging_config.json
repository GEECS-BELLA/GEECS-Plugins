{
    "version": 1, 
    "formatters": {
        "time_source_level_message": {
            "format": "{asctime}\t{name}:{levelname}\t{message}",
            "style": "{",
            "validate": false
        }
    },
    "filters": {},
    "handlers": {
        "livepostprocessing_file_handler": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "time_source_level_message",
            "filename": "log/livepostprocessing.log"
        },
        "scan_analyzer_file_handler": {
            "class": "logging.FileHandler",
            "level": "INFO",
            "formatter": "time_source_level_message",
            "filename": "log/scan_analyzer.log"
        }
    },
    "loggers": {
        "livepostprocessing": {
            "level": "INFO",
            "propagate": false,
            "handlers": [
                "livepostprocessing_file_handler"
            ]
        },
        "scan_analyzer": {
            "level": "INFO",
            "propagate": false,
            "handlers": [
                "scan_analyzer_file_handler"
            ]
        }
    },
    "root": {
        "level": "ERROR",
        "handlers": []
    }
}
